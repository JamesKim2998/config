# basic commands
alias rm="trash"

# vim
alias v="nvim"
alias ve="nvim $HOME/.config/nvim/init.vim"

# git
alias gdo="git diff origin/master"
alias gup="git pull --rebase --autostash --prune"

# zshrc
alias ze="nvim $HOME/.zshrc"
alias zwe="nvim $HOME/.zshrc_workspace"
alias zs="source $HOME/.zshrc"

function gshowcs {
    git show $1 -- '*.cs'
}

# cd somewhere
alias dev="cd $HOME/Develop"
alias dn="cd $HOME/Downloads"

