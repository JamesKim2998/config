	email = eodnjs2998@gmail.com
[credential]
	helper = osxkeychain
[core]
	editor = nvim
	quotepath = off
	excludesfile = ~/.gitignore_global
	autocrlf = input
[pull]
	rebase = true
[merge]
	ff = false
[rebase]
	autoStash = true
[lfs]
	concurrenttransfers = 64
[filter "lfs"]
	process = git-lfs filter-process
	required = true
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
